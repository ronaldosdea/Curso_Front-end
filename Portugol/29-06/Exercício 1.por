programa {
  funcao inicio() {
    inteiro numero1, numero2, soma

    escreva ("\n Seja bem-vindo a Calculadora Online")

    escreva ("\n Digite o primeiro número: ")
    leia (numero1)

    escreva ("\n Digite o segundo número: ")
    leia (numero2)

    soma = numero1 + numero2

    escreva ("\n A soma é: ", soma)
    escreva ("\n Finish!")
  }
}
