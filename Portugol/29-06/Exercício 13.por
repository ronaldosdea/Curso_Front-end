programa {
  funcao inicio() {
  real altura, largura, resultado

  escreva("Vamos calcular a área de um retângulo em centímetros!")
  escreva("\n Qual a altura do retângulo? ")
  leia(altura)
  escreva("Qual a largura do retângulo? ")
  leia(largura)

  limpa()

  resultado = altura*largura

  escreva("\n A Área do retângulo é: ", resultado, "cm")

    
  }
}
